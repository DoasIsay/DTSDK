{
  "name": "test",
  "source": {
    "type": "kafka",
    "broker-list": "127.0.0.1:9092"
  },
  "sink": [
    {
      "name": "sink1",
      "type": "kafka",
      "broker-list": "127.0.0.1:9092"
    },
    {
      "name": "sink2",
      "type": "kafka",
      "broker-list": "127.0.0.1:9092"
    },
    {
      "name": "sink3",
      "type": "kafka",
      "broker-list": "127.0.0.1:9092"
    }
  ],
  "process": {
    "resid": {
      "inFields": [
        {"name": "field0", "type": "string"},
        {"name": "field1", "type": "string"}
      ],
      "outFields": [
        {"name": "field", "type": "string"}
      ],
      "functors": [
        {
          "name": "Concat",
          "config": {
            "inFields": ["field0", "field1"],
            "outFields": ["field"],
            "split": "_"
          }
        },
        {
          "name": "Substr",
          "config": {
            "inFields": ["field"],
            "outFields": ["field"],
            "start": "1",
            "end": "3",
            "split": "_"
          }
        },{
          "name": "DictMap",
          "config": {
            "inFields": ["field"],
            "outFields": ["field1"],
            "namespace": "dict0",
            "dictionary": {"23": ["78"],"78": ["96"]}
          }
        },
        {
          "name": "Dispatch",
          "config": {
            "inFields": ["field0"],
            "rule": {
              "cat": "sink1",
              "fish": "sink2",
              "dog": "sink3"
            }
          }
        }
      ]
    }
  }
}